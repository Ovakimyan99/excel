{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "extends": ["eslint:recommended", "google"],
  "rules": {
    "linebreak-style": "off",
    "semi": "off",
    "object-curly-spacing":"off",
    "arrow-parens":"off",
    "comma-dangle": "off",
    "require-jsdoc": "off",
    "eol-last": "off",
    "operator-linebreak": "off",
    "no-debugger":"off"
  }
}